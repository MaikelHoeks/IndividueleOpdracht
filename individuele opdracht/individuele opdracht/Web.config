<configuration>
  <system.web>
    <authentication mode="Forms">
      <forms loginUrl="Inlogsysteem.aspx"  name="adAuthCookie" defaultUrl="~/Home.aspx" path ="/">
      </forms>
    </authentication>
    <authorization>
      <deny users="?"/>
      <allow users ="*"/>
    </authorization>
    <compilation debug="true" targetFramework="4.5"/>
    <httpRuntime targetFramework="4.5"/>
  </system.web>
<location path ="Home.aspx">
    <system.web>
      <authorization>
        <deny users ="?"/>
      </authorization>
    </system.web>
  </location>
</configuration>
